{
  "name": "AcciMotoSparePartsApp",
  "bundleID": "fr.accimoto.app",
  "version": "1.0.0",
  "private": true,
  "story": false,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "iphone": "react-native run-ios",
    "android": "adb reverse tcp:8081 tcp:8081; react-native run-android",
    "clean": "yarn --reset-cache; rimraf node_modules .cache/* cache/* coverage/*",
    "pretty": "eslint components --fix; prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "eslint": "^5.10.0",
    "fbjs-scripts": "^1.0.1",
    "react": "^16.6.3",
    "react-devtools-core": "^3.4.3",
    "react-dom": "^16.6.3",
    "react-is": "^16.6.3",
    "react-native": "^0.57.8",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera",
    "react-native-camera-kit": "^6.3.1",
    "react-native-cli": "^2.0.1",
    "react-native-image-picker": "^0.27.2",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-navigation": "^2.18.3",
    "rn-fetch-blob": "^0.10.14"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-react-native": "^4.0.1",
    "babel-runtime": "^6.26.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "metro-react-native-babel-preset": "^0.51.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "styled-components": "^4.1.3"
  },
  "pretty": "prettier --semi false --print-width 100 --single-quote --trailing-comma all --write",
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/?(*.)test.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setup.js",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "."
    ],
    "cacheDirectory": "./cache",
    "coveragePathIgnorePatterns": [
      "./app/utils/vendor"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|react-clone-referenced-element|react-navigation)"
    ]
  }
}
